{
  "manifest_version": 3,
  "name": "Easy Clean Beta 1",
  "description": "__MSG_extDesc__",
  "default_locale": "en",
  "author": "Raz1ner",
  "version": "1.0.0",
  "action": {
    "default_popup": "html/popup.html",
    "default_icon": "icon/icon.png"
  },
  "icons": {
    "128": "icon/icon.png"
  },
  "background": {
    "service_worker": "js/background.js"
  },
  "permissions": [
    "cookies",
    "storage",
    "windows",
    "browsingData"
  ],
  "host_permissions": ["*://*/*"]
}
